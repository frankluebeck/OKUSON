def getpage(host, path):
   sys.stdout.write('.')
   sys.stdout.flush()
   h = httplib.HTTP(host)
   h.putrequest('GET', path)
   h.endheaders()
   errcode, errmsg, headers = h.getreply()
   if errcode != 200:
     print 'ERROR: ',errcode
   f = h.getfile()
   data = f.read() # Get the raw data
   f.close()
   return data

